#include <stdio.h>

int main()
{
    float valorReal , valorDolar;
    
    //    real valorReal, valorDolar
    // $1 = R$5,30
    
    
    printf("Digite o Valor a ser convertido : $ \n ", valorReal);
    scanf("%f" , &valorReal);
    
    valorDolar = valorReal * 5.3;

    printf("\nValor Convertido R$ : %.2f " , valorDolar  );

   system ("PAUSE");
}
